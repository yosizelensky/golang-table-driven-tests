{
  "name": "golang-table-driven-tests",
  "displayName": "Go Table Driven Tests CodeLens Provider",
  "description": "Go Table Driven Tests CodeLens Provider for Visual Studio Code",
  "publisher": "yosizelensky",
  "version": "2.1.0",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/yosizelensky/golang-table-driven-tests"
  },
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:go"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "go-test-decorator.runTest",
        "title": "Run Go Test"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "node test/run_tests.js",
    "test:vscode": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.93.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "^9.16.0",
    "typescript": "^5.7.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}